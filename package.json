  "scripts": {
    "script:check-env": "ts-node ./bin/env/check-env.ts",
    "script:seed-generator": "ts-node ./bin/database/seed-generator.ts",
    "build:prisma": "yarn prisma generate && yarn prisma format",
    "build:nest": "nest build",
    "build": "rimraf dist && run-s build:*",
    "test": "jest --config ./jest.config.ts",
    "test:e2e": "jest --config ./jest-e2e.config.ts",
    "start:dev": "yarn script:check-env && nest start --watch",
    "sls:offline": "yarn script:check-env && yarn build && sh ./bin/serverless/sls-offline.sh",
    "sls:online": "yarn script:check-env && yarn build && sh ./bin/serverless/sls-deploy.sh",
    "migrate:deploy": "yarn prisma migrate deploy --preview-feature",
    "migrate:dev": "yarn prisma migrate dev --preview-feature",
    "studio": "yarn prisma studio"
  },
  "dependencies": {
