    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "serverless-offline": "^8.4.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-warmup": "^6.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.3",
    "ts-morph": "^13.0.3",
