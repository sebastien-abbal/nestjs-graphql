{
  "name": "nestjs-graphql",
  "version": "0.0.1",
  "description": "A graphql nestjs project",
  "author": "ABBAL SÃ©bastien",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "check:env": "ts-node ./bin/env/check-env.ts",
    "build:nest": "nest build",
    "build": "rimraf dist && run-s build:*",
    "test": "jest",
    "typeorm": "ts-node -r tsconfig-paths/register -T ./node_modules/typeorm/cli.js --config ./bin/database/pg.config.ts",
    "migrate:generate": "yarn build && yarn typeorm migration:generate -d generated/migrations -n ",
    "migrate:run": "yarn build && yarn typeorm migration:run",
    "start:dev": "yarn check:env && nest start --watch",
    "sls:offline": "yarn check:env && yarn build && sh ./bin/serverless/sls-offline.sh",
    "sls:online": "yarn check:env && yarn build && sh ./bin/serverless/sls-deploy.sh"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "@vendia/serverless-express": "^4.5.3",
    "apollo-server-express": "^3.6.3",
    "apollo-server-plugin-base": "^3.5.1",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "env-var": "^7.1.1",
    "graphql": "^15",
    "graphql-query-complexity": "^0.11.0",
    "nestjs-pino": "^2.5.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pino-http": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.3",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
